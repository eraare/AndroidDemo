<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#0cb29d"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="30dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:background="@drawable/bg_pc"
                android:gravity="center_horizontal"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingLeft="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="usb"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="k1"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="k2"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="k3"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="k4"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0.0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iv_all"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <ImageView
                        android:id="@+id/iv_usb_l"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_usb_left_selector" />

                    <ImageView
                        android:id="@+id/iv_socket_l_k1"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_left_selector" />

                    <ImageView
                        android:id="@+id/iv_socket_l_k2"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_left_selector" />

                    <ImageView
                        android:id="@+id/iv_socket_l_k3"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_left_selector" />

                    <ImageView
                        android:id="@+id/iv_socket_l_k4"
                        android:layout_width="match_parent"
                        android:layout_height="0.0dp"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_left_selector" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/delay_time_text_usb"
                        android:layout_width="0.0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/clock_off"
                        android:gravity="center"
                        android:text="@string/notime"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <ImageView
                        android:id="@+id/iv_usb_r"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <TextView
                        android:id="@+id/tv_usb_name"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawablePadding="5.0dip"
                        android:drawableRight="@drawable/edit_pencil_white"
                        android:gravity="center"
                        android:text="手机"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/delay_time_text_k1"
                        android:layout_width="0.0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/clock_on"
                        android:gravity="center"
                        android:text="@string/notime"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <ImageView
                        android:id="@+id/iv_socket_r_k1"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <TextView
                        android:id="@+id/tv_k1_name"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawablePadding="5.0dip"
                        android:drawableRight="@drawable/edit_pencil_white"
                        android:gravity="center"
                        android:text="手机"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/delay_time_text_k2"
                        android:layout_width="0.0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/clock_on"
                        android:gravity="center"
                        android:text="@string/notime"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <ImageView
                        android:id="@+id/iv_socket_r_k2"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <TextView
                        android:id="@+id/tv_k2_name"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawablePadding="5.0dip"
                        android:drawableRight="@drawable/edit_pencil_white"
                        android:gravity="center"
                        android:text="手机"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/delay_time_text_k3"
                        android:layout_width="0.0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/clock_on"
                        android:gravity="center"
                        android:text="@string/notime"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <ImageView
                        android:id="@+id/iv_socket_r_k3"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <TextView
                        android:id="@+id/tv_k3_name"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawablePadding="5.0dip"
                        android:drawableRight="@drawable/edit_pencil_white"
                        android:gravity="center"
                        android:text="手机"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/delay_time_text_k4"
                        android:layout_width="0.0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/clock_on"
                        android:gravity="center"
                        android:text="@string/notime"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />

                    <ImageView
                        android:id="@+id/iv_socket_r_k4"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:scaleType="center"
                        android:src="@drawable/new_socket_selector" />

                    <TextView
                        android:id="@+id/tv_k4_name"
                        android:layout_width="0.0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawablePadding="5.0dip"
                        android:drawableRight="@drawable/edit_pencil_white"
                        android:gravity="center"
                        android:text="手机"
                        android:textColor="#ffffff"
                        android:textSize="14.0sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>